#include <stdio.h>
main()
{
   int x,ge,shi,qian,wan;
   printf("\n请输入一个5位数：");
   scanf("%d",&x);
   if(x>10000 && x<100000)
   {
	   wan=x/10000;
	   qian=x%10000/1000;
	   shi=x%100/10;
	   ge=x%10;
	   if(ge==wan && shi==qian)
		   printf("该数是回文数！\n");
	   else printf("该数不是回文数！\n");
   }
   else
   {
	   printf("抱歉，该数不是一个五位数！\n");
	   exit(1);
   } 

}
